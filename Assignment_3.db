# Solution a

SELECT Movie.title, COUNT(DISTINCT M_Cast.PID) AS cast_size
FROM M_Cast, Movie
WHERE M_Cast.MID = Movie.MID
GROUP BY M_Cast.MID
ORDER BY cast_size DESC
LIMIT 1;


# Solution b

SELECT Movie.year, COUNT(*) AS number_of_movies 
FROM Movie WHERE Movie.MID IN (
                                SELECT M_Cast.MID 
                                FROM M_Cast, Person 
                                WHERE M_Cast.PID = Person.PID AND Person.Gender = 'Female');
